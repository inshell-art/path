        "safe_confirm",
        "safe_execute"
      ],
      "required_checks": [
        "chain_id",
        "rpc_allowlist",
        "signer_allowlist",
        "target_identity",
        "preconditions",
        "simulate",
        "postconditions"
      ],
      "safe_rules": {
        "treasury": {
          "quorum": 2,
          "signers": [
            "SEPOLIA_TREASURY_SW_A",
            "SEPOLIA_TREASURY_HW_B"
