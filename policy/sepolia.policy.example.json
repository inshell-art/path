    "SEPOLIA_DEPLOY_SW_A": {
      "type": "keystore",
      "address_env": "SEPOLIA_DEPLOY_ADDRESS",
      "keystore_env": "SEPOLIA_DEPLOY_KEYSTORE_JSON"
