      "required_checks": [
        "chain_id",
        "rpc_allowlist",
        "signer_allowlist",
        "target_identity",
        "preconditions",
        "postconditions"
      ]
    },
    "govern": {
      "writes": true,
      "allowed_signers": [
        "SEPOLIA_GOV_SW_A",
        "SEPOLIA_GOV_HW_B",
        "SEPOLIA_TREASURY_SW_A",
        "SEPOLIA_TREASURY_HW_B"
      ],
      "allowed_ops": [
        "safe_propose",
        "safe_confirm",
        "safe_execute"
      ],
      "required_checks": [
        "chain_id",
        "rpc_allowlist",
        "signer_allowlist",
        "target_identity",
        "preconditions",
        "simulate",
        "postconditions"
      ],
      "safe_rules": {
        "gov": {
          "quorum": 2,
          "signers": [
            "SEPOLIA_GOV_SW_A",
            "SEPOLIA_GOV_HW_B"
          ]
        },
        "treasury": {
          "quorum": 2,
          "signers": [
            "SEPOLIA_TREASURY_SW_A",
