      "fee_policy": {
        "max_fee_per_gas_gwei": "<SET_ME>",
        "max_priority_fee_per_gas_gwei": "<SET_ME>",
        "enforce_eip1559": true
      }
    },
    "handoff": {
      "writes": true,
      "allowed_signers": [
        "SEPOLIA_DEPLOY_SW_A",
        "SEPOLIA_GOV_SW_A",
        "SEPOLIA_GOV_HW_B"
      ],
      "allowed_ops": [
        "grant_role",
        "revoke_role",
        "transfer_ownership",
        "accept_ownership",
        "safe_propose",
        "safe_confirm",
        "safe_execute"
