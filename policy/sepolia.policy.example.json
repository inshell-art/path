        "safe_propose",
        "safe_confirm",
        "safe_execute"
      ],
      "required_checks": [
        "chain_id",
        "rpc_allowlist",
        "signer_allowlist",
        "target_identity",
        "preconditions",
        "postconditions"
      ]
    }
